<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.hotelproject.repository.UserRepository">
    <resultMap id="userMap" type="com.example.hotelproject.domain.User">
        <result property="test_id" column="userId"/>
        <result property="userName" column="userName"/>
        <result property="phoneNumber" column="phoneNumber"/>
        <result property="age" column="age"/>
    </resultMap>

    <insert id="saveUser">
        INSERT INTO testTable (test_id, userName, phoneNumber, age)
        VALUES (#{userId},#{userName},#{phoneNumber}, #{age})
    </insert>

</mapper>



